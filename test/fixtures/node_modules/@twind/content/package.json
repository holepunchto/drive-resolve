{
  "name": "@twind/content",
  "version": "0.1.0",
  "description": "A twind extension that provides a CSS content property directive.",
  "keywords": [
    "twind",
    "extension",
    "twind-extension",
    "plugin",
    "twind-plugin",
    "tailwind",
    "tw-in-js",
    "tailwind-in-js"
  ],
  "bugs": "https://github.com/tw-in-js/twind-content/issues",
  "repository": "github:tw-in-js/twind-content",
  "license": "MIT",
  "contributors": [
    "Sascha Tandel (https://github.com/sastan)"
  ],
  "engines": {
    "node": ">=10.13"
  },
  "main": "./content.cjs",
  "exports": {
    ".": {
      "module": "./content.js",
      "script": "./content.umd.js",
      "types": "./content.d.ts",
      "require": "./content.cjs",
      "node": "./content.mjs",
      "default": "./content.js"
    },
    "./package.json": "./package.json"
  },
  "peerDependencies": {
    "twind": ">=0.14.4",
    "typescript": "^4.1.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "module": "./content.js",
  "unpkg": "./content.umd.js",
  "types": "./content.d.ts"
}